    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Hello Releasing'
            }
        }
    }
}
